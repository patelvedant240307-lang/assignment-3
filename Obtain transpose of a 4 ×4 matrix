#include <stdio.h>
int main() {
int matrix[4][4]={
      {2,7,11,0},
      {3,19,22,8},
      {5,21,4,16},
      {24,12,1,31}
};
int transpose[4][4];
int i,j;
for(i=0;i<4;i++) {
    for(j=0;j<4;j++) {
        transpose[j][i]=matrix[i][j];
        }
    }
printf("transpose of matrix:\n");
for(i=0;i<4;i++)
{
    for(j=0;j<4;j++) {
        printf("%d\t",transpose[i][j]);
        }
printf("\n");
}
return 0;
}
