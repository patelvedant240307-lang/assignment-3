#include <stdio.h>
int main() {
int i, newnum,arr[11];
printf("enter 10 values=\n");
    for(i=0;i<10;i++)
    {
     scanf("%d", &arr[i]);
    }
printf("enter new number for insert at last position=");
scanf("%d", &newnum);
arr[10] = newnum;
printf("updated array=\n");
for(i=0;i<11;i++)
    {
     printf("%d ", arr[i]);
    }
return 0;
}
